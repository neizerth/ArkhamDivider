{
	"rewrites": [
		{
			"source": "/((?!assets/|favicon|images/|coi-serviceworker).*)",
			"destination": "/index.html"
		},
		{
			"source": "/arkham-icons/:path*",
			"destination": "https://neizerth.github.io/ArkhamDividerData/fonts"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Cross-Origin-Opener-Policy",
					"value": "same-origin"
				},
				{
					"key": "Cross-Origin-Embedder-Policy",
					"value": "credentialless"
				},
				{
					"key": "X-Content-Type-Options",
					"value": "nosniff"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				}
			]
		},
		{
			"source": "/assets/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}
